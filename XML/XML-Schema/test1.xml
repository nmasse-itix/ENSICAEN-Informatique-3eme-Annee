<?xml version="1.0" ?>
<component-list 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="test1.xsd">

  <!-- In this example, two components have pins with the same id 
       ("vcc" and "gnd"). This is valid. -->

  <component id="c1">
    <pin id="vcc" />
    <pin id="gnd" />
    <pin id="in-1" />
    <pin id="in-2" />
    <pin id="out-1" />
  </component>
  
  <component id="c2">
    <pin id="vcc" />
    <pin id="gnd" />
    <pin id="pin-a" />
    <pin id="pin-b" />
  </component>
</component-list>
